{
  "name": "kano-web-playground",
  "private": true,
  "scripts": {
    "start": "yarn run generateFiles && yarn run serve",
    "serve": "babel-node tools/server.js",
    "lint": "eslint webpack.config.* app tools --color --fix",
    "pretty": "prettier --write \"**/*.{js,scss}\"",
    "generateIndexFiles": "run-p generateActionIndexFiles generateComponentIndexFiles generateReducerIndexFiles generateServiceIndexFiles generateSelectorIndexFiles",
    "generateActionIndexFiles": "babel-node tools/generateActionIndexFiles.js",
    "generateComponentIndexFiles": "babel-node tools/generateComponentIndexFiles.js",
    "generateServiceIndexFiles": "babel-node tools/generateServiceIndexFiles.js",
    "generateSelectorIndexFiles": "babel-node tools/generateSelectorIndexFiles.js",
    "generateReducerIndexFiles": "babel-node tools/generateReducerIndexFiles.js",
    "generateRootReducer": "babel-node tools/generateRootReducer.js",
    "generateFiles": "run-p generateIndexFiles generateRootReducer",
    "webpack": "webpack --config webpack.config.prod.babel.js --bail",
    "clean": "rimraf ./lib",
    "build": "yarn run generateFiles && yarn run webpack"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --color --fix",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@synvox/rehook": "^0.0.14",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.18.0",
    "class-autobind": "^0.1.4",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.2",
    "history": "^4.9.0",
    "immer": "^2.1.5",
    "immutable": "^4.0.0-rc.12",
    "jquery": "^3.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-async-await": "^1.0.1",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semantic-ui-react": "^0.86.0",
    "toastr": "^2.1.4",
    "truefit-http-utils": "^1.0.2",
    "truefit-redux-utils": "^1.0.1",
    "universal-cookie": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/helpers": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@hot-loader/react-dom": "16.8.6",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react-hmre": "^1.1.1",
    "browser-sync": "^2.26.3",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-extended": "^0.11.1",
    "jest-haste-map": "^24.7.1",
    "jest-resolve": "^24.7.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "node-dir": "^0.1.17",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.3",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-stylish": "^0.1.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
