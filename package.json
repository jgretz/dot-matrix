{
  "name": "kanoplayground",
  "version": "0.0.1",
  "description": "messing around with the kano pixel",
  "main": "src/index.js",
  "repository": "https://github.com/jgretz/kano-playground",
  "author": "Josh Gretz",
  "license": "MIT",
  "scripts": {
    "build": "node tools/build.js",
    "debug": "nodemon --exec babel-node --inspect --presets es2015 src/index.js --inspect",
    "nodemon": "NODE_ENV=DEV nodemon src/index.js --exec babel-node",
    "start": "babel-node src/index.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --color --fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "community-sdk": "KanoComputing/community-sdk#nodejs",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^5.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "shelljs": "^0.8.2"
  }
}
